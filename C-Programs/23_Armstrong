#include<stdio.h>
int main()
{
int first_term,second_term,thired_term,num,arm;
printf("Enter Three digite No : ");
scanf("%d",&num);
first_term  = num/100;
second_term = (num/10)%10;
thired_term = num%10;
//eg : 153 = 1^3 + 5^3 + 3^3
arm=first_term*first_term*first_term + second_term*second_term*second_term + thired_term*thired_term*thired_term;
if (num==arm)
{
printf("%d is Armstrong",num);
}
else
{
printf("%d is not Armstrong",num);
}
}  
